<h1>All Cars</h1>
<h3>@Model.CurrCategory</h3>
<div class="row mt-5 mb-2">
	@{
		foreach (var car in Model.AllCars)
		{
			<div class="col-lg-4">
				<img class="img-thumbnail" src="@car.Image" alt="@car.Name" />
				<h2>Model: @car.Name</h2>
				<p>Price: @car.Price.ToString("c")</p>
				<p><a class="btn btn-warning" href="#">More details</a></p>
			</div>
		}
	}
</div>

